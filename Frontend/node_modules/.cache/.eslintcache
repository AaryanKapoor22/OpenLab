oject/OpenLab/OpenLab/Frontend/src/components/LabManager.jsx",[],[],"/Users/matt/Documents/Spring24/SER341FullStack/TermProject/OpenLab/OpenLab/Frontend/src/components/LabScheduling.jsx",["238","239"],[],"/Users/matt/Documents/Spring24/SER341FullStack/TermProject/OpenLab/OpenLab/Frontend/src/components/labScheduling.jsx",["240"],[],"import React, { useEffect, useState, useContext } from 'react';\nimport { toast } from 'react-toastify';\nimport 'react-toastify/dist/ReactToastify.css';\nimport TableHead from './tableHead';\nimport TableBody from './tableBody';\nimport { LabContext } from './labContext';\nimport { getMissedLabs, getOpenLabs, registerForLab } from '../services/LabSchedulingService';\n\nconst Labs = () => {\n  const [activeTab, setActiveTab] = useState('missed');\n  const [sessions, setSessions] = useState([]); \n  const { registeredSessions, setRegisteredSessions } = useContext(LabContext);\n\n  useEffect(() => {\n    const fetchData = async () => {\n      let data;\n      if (activeTab === 'missed') {\n        data = await getMissedLabs();\n      } else if (activeTab === 'open') {\n        data = await getOpenLabs();\n      }\n      setSessions(data);\n    };\n\n    fetchData();\n  }, [activeTab]);\n\n  const handleRegister = async (sessionId, userId) => {\n    // Check if the user is already registered for the lab\n    if (registeredSessions.includes(sessionId)) {\n      // If the user is already registered, display a message and return early\n      console.log(\"You're already registered for this lab.\");\n      toast.error(\"You're already registered for this lab.\");\n      return;\n    }\n  \n    // If the user is not already registered, attempt to register them for the lab\n    try {\n      await registerForLab(sessionId, userId);\n      setRegisteredSessions(prevRegisteredSessions => [...prevRegisteredSessions, sessionId]);\n      toast.success(\"You've successfully registered for the lab.\");\n    } catch (ex) {\n      toast.error(\"An error occurred while registering for the lab.\");\n    }\n  };\n\n  const isLabRegistered = (labId) => {\n    return registeredSessions.includes(labId);\n  };\n\n  return (\n    <div className=\"container-fluid\">\n      <ul className=\"nav nav-pills justify-content-center\" style={{ margin: '10px' }}>\n        <li className=\"nav-item\">\n          <button className={`nav-link ${activeTab === 'missed' ? 'active' : ''}`} onClick={() => setActiveTab('missed')}>Missed</button>\n        </li>\n        <li className=\"nav-item\">\n          <button className={`nav-link ${activeTab === 'open' ? 'active' : ''}`} onClick={() => setActiveTab('open')}>Open</button>\n        </li>\n      </ul>\n      <TableHead />\n      <TableBody \n        sessions={sessions}\n        onRegister={handleRegister}\n        setDisplay={true}\n        showRegister={true}\n        showDelete={false} \n        registeredSessions={registeredSessions}\n      />\n    </div>\n  );\n};\n\nexport default Labs;\n","/Users/matt/Documents/Spring24/SER341FullStack/TermProject/OpenLab/OpenLab/Frontend/src/components/absenceTracking.jsx",[],[],"/Users/matt/Documents/Spring24/SER341FullStack/TermProject/OpenLab/OpenLab/Frontend/src/components/protectedRoute.jsx",[],[],"/Users/matt/Documents/Spring24/SER341FullStack/TermProject/OpenLab/OpenLab/Frontend/src/components/UserContext.jsx",[],[],"/Users/matt/Documents/Spring24/SER341FullStack/TermProject/OpenLab/OpenLab/Frontend/src/services/openLabService.jsx",[],[],"/Users/matt/Documents/Spring24/SER341FullStack/TermProject/OpenLab/OpenLab/Frontend/src/components/labContext.jsx",[],[],"/Users/matt/Documents/Spring24/SER341FullStack/TermProject/OpenLab/OpenLab/Frontend/src/components/recipeDetails.jsx",[],[],"/Users/matt/Documents/Spring24/SER341FullStack/TermProject/OpenLab/OpenLab/Frontend/src/components/labs.jsx",["241"],[],"import React, { useEffect, useState } from 'react';\nimport TableHead from '../components/tableHead';\nimport TableBody from '../components/tableBody';\nimport auth from \"../services/authService\";\nimport { getOpenLabs } from '../services/openLabs';\nimport { getMissedLabs } from '../services/missedLabs';\n\nconst Labs = () => {\n  // https://stackoverflow.com/questions/58227499/is-there-a-way-of-using-the-usestate-hook-to-toggle-classname-in-react\n  const [activeTab, setActiveTab] = useState('missed');\n  // for the list of labs \n  const [sessions, setSessions] = useState([]); \n  // for modal logic: https://www.geeksforgeeks.org/how-to-pass-data-to-a-react-bootstrap-modal/\n  const [modalVisible, setModalVisible] = useState(false);\n  const [selectedSession, setSelectedSession] = useState(null);\n\n\n  useEffect(() => {\n      // only registered users can access this page (uncomment when whole application is ready)\n\n    // if (!auth.getCurrentUser()) {\n    //   console.log(\"no user\");\n    //   window.location = \"/login\";\n    // } else {\n      // sets the informtion based on whether user wants to access a missed or open lab\n      async function fetchData() {\n        const data = activeTab === 'missed' ? await getMissedLabs() : await getOpenLabs();\n        setSessions(data);\n      }\n      fetchData();\n   // }\n  }, [activeTab]); \n\n  // logic for modal that displays the lab details user signed up for\n\n  // onClick for register button \n  const handleRegister = (session) => {\n    setSelectedSession(session);\n    setModalVisible(true);\n  };\n  // for if user cancels registration\n  const handleCloseModal = () => {\n    setModalVisible(false);\n  };\n\n  // confirms registration\n  const handleConfirmation = () => {\n    // can be something as simple as a adding alert \n    alert(\"Registration Confirmed!\");\n    setModalVisible(false);\n  }\n\n  return (\n    <div className=\"container-fluid\">\n      <ul className=\"nav nav-pills justify-content-center\" style={{ margin: '10px' }}>\n        {/* https://stackoverflow.com/questions/72707446/use-ternary-operator-to-change-classname-in-react */}\n        <li className=\"nav-item\">\n          <button className={`nav-link ${activeTab === 'missed' ? 'active' : ''}`} onClick={() => setActiveTab('missed')}>Missed</button>\n        </li>\n        <li className=\"nav-item\">\n          <button className={`nav-link ${activeTab === 'open' ? 'active' : ''}`} onClick={() => setActiveTab('open')}>Open</button>\n        </li>\n      </ul>\n      <TableHead />\n      <TableBody activeTab={activeTab} sessions={sessions} onClick={handleRegister}  setDisplay={true} />\n         {/* Based on the modal and the exact lab session clicked, display the corresponding information \n         https://getbootstrap.com/docs/5.0/components/modal/#fullscreen-modal\n         */}\n      {modalVisible && selectedSession && (\n        <div className=\"modal show\" style={{ display: 'block' }}>\n          <div className=\"modal-dialog\">\n            <div className=\"modal-content\">\n              <div className=\"modal-header\">\n                <h5 className=\"modal-title\">{selectedSession.title}</h5>\n                <button type=\"button\" className=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\" onClick={handleCloseModal}></button>\n              </div>\n              <div className=\"modal-body\">\n                <p>Date: {selectedSession.date}</p>\n                <p>Time: {selectedSession.time}</p>\n                <p>Instructor: {selectedSession.instructor}</p>\n                <p>Room: {selectedSession.room}</p>\n                <strong>Are you sure you wish to confirm your booking?</strong>\n              </div>\n              <div className=\"modal-footer\">\n                <button type=\"button\" className=\"btn btn-secondary\" onClick={handleCloseModal}>Close</button>\n                <button type=\"button\" className=\"btn btn-primary\" onClick={handleConfirmation}>Confirm Registration</button>\n              </div>\n            </div>\n          </div>\n        </div>\n      )}\n    </div>\n  );\n};\n\nexport default Labs;\n","/Users/matt/Documents/Spring24/SER341FullStack/TermProject/OpenLab/OpenLab/Frontend/src/components/labDashboard.jsx",[],[],"/Users/matt/Documents/Spring24/SER341FullStack/TermProject/OpenLab/OpenLab/Frontend/src/services/recipesService.jsx",[],[],"/Users/matt/Documents/Spring24/SER341FullStack/TermProject/OpenLab/OpenLab/Frontend/src/services/missedLabs.jsx",[],[],"/Users/matt/Documents/Spring24/SER341FullStack/TermProject/OpenLab/OpenLab/Frontend/src/services/openLabs.jsx",[],[],"/Users/matt/Documents/Spring24/SER341FullStack/TermProject/OpenLab/OpenLab/Frontend/src/services/labManagerInfo.jsx",[],[],"/Users/matt/Documents/Spring24/SER341FullStack/TermProject/OpenLab/OpenLab/Frontend/src/components/labAdderForm.jsx",[],[],{"ruleId":"242","severity":1,"message":"243","line":1,"column":8,"nodeType":"244","messageId":"245","endLine":1,"endColumn":17},{"ruleId":"242","severity":1,"message":"246","line":3,"column":8,"nodeType":"244","messageId":"245","endLine":3,"endColumn":14},{"ruleId":"242","severity":1,"message":"247","line":4,"column":8,"nodeType":"244","messageId":"245","endLine":4,"endColumn":11},{"ruleId":"248","severity":2,"message":"249","line":12,"column":22,"nodeType":"244","endLine":12,"endColumn":33,"suppressions":"250"},{"ruleId":"251","severity":1,"message":"252","line":26,"column":1,"nodeType":"253","endLine":32,"endColumn":3},{"ruleId":"242","severity":1,"message":"254","line":2,"column":8,"nodeType":"244","messageId":"245","endLine":2,"endColumn":12},{"ruleId":"242","severity":1,"message":"255","line":3,"column":10,"nodeType":"244","messageId":"245","endLine":3,"endColumn":16},{"ruleId":"242","severity":1,"message":"256","line":8,"column":7,"nodeType":"244","messageId":"245","endLine":8,"endColumn":18},{"ruleId":"251","severity":1,"message":"252","line":11,"column":1,"nodeType":"253","endLine":14,"endColumn":3},{"ruleId":"242","severity":1,"message":"257","line":4,"column":8,"nodeType":"244","messageId":"245","endLine":4,"en